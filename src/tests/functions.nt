print "--- TEST: Functions ---";

function int add(int a, int b) {
    return a + b;
}

function string greet(string name) {
    return "Hello " + name;
}

// Recursion Test
function int factorial(int n) {
    if (n <= 1) return 1;
    return n * factorial(n - 1);
}

print "Add(5,5): " + add(5, 5);
print greet("Sensei");
print "Factorial(5) should be 120: " + factorial(5);